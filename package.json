{
  "name": "nextjs-with-typescript",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze-bundle": "ANALYZE=true next build"
  },
  "dependencies": {
    "@emotion/cache": "11.10.5",
    "@emotion/react": "11.10.5",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.5",
    "@mui/icons-material": "5.10.15",
    "@mui/material": "5.10.15",
    "@next/font": "13.0.5",
    "@vercel/og": "^0.0.21",
    "cheerio": "^1.0.0-rc.12",
    "date-fns": "^2.29.3",
    "immer": "^9.0.16",
    "microcms-js-sdk": "^2.3.2",
    "next": "13.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "use-immer": "^0.8.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.1.1",
    "@types/cheerio": "^0.22.31",
    "@types/date-fns": "^2.6.0",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "eslint": "8.22.0",
    "eslint-config-next": "13.0.5",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.8.0",
    "typescript": "4.9.3"
  },
  "eslintConfig": {
    "extends": [
      "next/core-web-vitals",
      "prettier"
    ],
    "rules": {
      "react/display-name": "off",
      "import/order": [
        "error",
        {
          "groups": [
            "builtin",
            "external",
            "internal",
            "parent",
            "sibling",
            "index",
            "object"
          ],
          "newlines-between": "always",
          "alphabetize": {
            "order": "asc",
            "caseInsensitive": true
          }
        }
      ],
      "import/default": "off",
      "eol-last": [
        "error",
        "always"
      ],
      "react-hooks/exhaustive-deps": "off",
      "curly": "error"
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": true,
    "printWidth": 120
  }
}
